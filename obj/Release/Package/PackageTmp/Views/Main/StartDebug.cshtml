      <div id="divMainTabMenu"><ul id='ulMainMenu'>
         <li><a href="#tabAccidents" data-ctrl='{"CallFunc":"CallServer("", oGLOBAL.Start.fnSetNewData, {"Ctrl":ui.panel.id}, "/Main/"+ui.panel.id, "json");"}'>
            <span>ĮVYKIAI</span></a></li>
